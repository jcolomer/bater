<%= erb :nav %>
<div class="container">
  <div class="gravatar">
    <% @hash = Digest::MD5.hexdigest(@user_email) %>
    <img src="http://www.gravatar.com/avatar/<%= @hash %>"/>
  </div>

  <ul class="list">
  <% @followers_squeaks.each do |squeak| %>
    <li align="center"><%= squeak %></li><hr>
  <% end %>
  </ul>

</div>
